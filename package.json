{
  "name": "casl-shop",
  "private": true,
  "version": "0.0.0",
  "description": "CASL shop",
  "arc": {
    "app": "casl-shop",
    "http": [
      {
        "/commands/order": {
          "method": "post",
          "src": "/commands/order"
        }
      },
      {
        "/commands/order": {
          "method": "options",
          "src": "/cors"
        }
      },
      {
        "/commands/calcShippingCost": {
          "method": "post",
          "src": "/commands/calcShippingCost"
        }
      },
      {
        "/commands/calcShippingCost": {
          "method": "options",
          "src": "/cors"
        }
      },
      {
        "/products": {
          "method": "get",
          "src": "/products/findAll"
        }
      },
      {
        "/products": {
          "method": "options",
          "src": "/cors"
        }
      }
    ]
  },
  "scripts": {
    "start": "npx sandbox"
  },
  "dependencies": {
    "@architect/functions": "^3.13.9",
    "@architect/sandbox": "^3.4.0",
    "axios": "^0.21.4",
    "tiny-json-http": "^7.3.0"
  }
}
